<!-- HEADER line -->
<div class="container">
	<div class="row pb-2 mt-4 mb-2 border-bottom">
		<div class="col">
			<h1>Focal Adhesion Analysis Server - Submitted Experiments</h1>
		</div>
	</div>

	<div class="row">
		<div class="col">
			<table class="table">
				<thead>
					<tr>
						<td>Experiment ID</td>
						<td>Status</td>
					</tr>
				</thead>
				<tbody>
					[% FOREACH exp IN exp_list %]
					<tr class=[% exp.markup %]>
						<td>[% exp.exp_id %]</td>
						<td>[% exp.exp_status %]</td>
					</tr>

					[% END %]
				</tbody>
			</table>
		</div>
	</div>
</div>
